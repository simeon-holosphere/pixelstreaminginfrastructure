{
	"log_folder": "logs",
	"log_level_console": "info",
	"log_level_file": "info",
	"streamer_port": "$STREAMER_PORT",
	"player_port": "$PLAYER_PORT",
	"sfu_port": "$SFU_PORT",
	"serve": true,
	"http_root": "D:\\PixelStreamingInfrastructure\\SignallingWebServer\\www",
	"homepage": "player.html",
	"https": false,
	"https_port": 443,
	"ssl_key_path": "certificates/client-key.pem",
	"ssl_cert_path": "certificates/client-cert.pem",
	"https_redirect": false,
	"rest_api": false,
	"peer_options": {
		"iceServers": [
			{
				"urls": ["stun:stun.l.google.com:19302"]
			}
		]
	},
	"log_config": true,
	"stdin": false,
	"console_messages": "verbose",
	"ngrok": {
		"enabled": true,
		"authToken": "$NGROK_AUTH_TOKEN",
		"basicAuth": {
			"username": "$NGROK_USERNAME",
			"password": "$NGROK_PASSWORD"
		}
	}
}